 "html": "{% set value = get_company_info() %}\r\n<div class=\"w-50\">\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-3 width-logo gj-ml-2\">\r\n            <h4 class=\"text-left \">\r\n                <img class=\"w-100 max-width-logo\" src=\"/assets/lpp/images/lpp-logo.png\" alt=\"Logo\">\r\n            </h4>\r\n        </div>\r\n        <div class=\"gj-col-8-5 fs-costomer gj-mx-2\">\r\n            <div class=\"my-2\">\r\n                <strong>{{value.company_name_en}}</strong>\r\n            </div>\r\n            <div class=\"my-2\">\r\n                <strong>{{value.company_name_th}}</strong>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-6\">\r\n                    <strong>Run Card No.</strong>\r\n                </div>\r\n                <div class=\"gj-col-6 text-right\"> \r\n                    {{doc.custom_runcard_no}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-6\">\r\n                    <strong>Run Card Date</strong>\r\n                </div>\r\n                <div class=\"gj-col-6 text-right\"> \r\n                    {{doc.posting_date}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-6\">\r\n                    <strong>Sequence</strong>\r\n                </div>\r\n                <div class=\"gj-col-6 text-right\"> \r\n                    {{doc.custom_sequence}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"section gj-p-1\">\r\n        <div class=\"gj-row gj-my-1\">\r\n            <div class=\"gj-col-7\">\r\n                <strong>Quantity on this Run Card</strong>\r\n            </div>\r\n            <div class=\"gj-col-5 text-right\"> \r\n                {{doc.custom_quantity_on_this_run_card}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Work Order No.</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.work_order}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-6\">\r\n                    <strong>Lot No</strong>\r\n                </div>\r\n                <div class=\"gj-col-6 text-right\"> \r\n                    {{doc.custom_lot_no}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Customer</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.custom_customer}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Item Name</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.custom_production_item_name}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Material</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.custom_material}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"section gj-p-1\">\r\n        <div class=\"gj-row gj-my-1\">\r\n            <div class=\"gj-col-5\">\r\n                <strong>Build Sheet No.</strong>\r\n            </div>\r\n            <div class=\"gj-col-7 text-right\"> \r\n                {{doc.custom_build_sheet_no}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Workstation</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.custom_workstation_details}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Machine</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.custom_machine}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Worker</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {% for item in doc.employee %}\r\n                        <div>\r\n                            {{item.employee}}\r\n                        </div>\r\n                    {% endfor %}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Shift</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.custom_shift}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>Start Date & Time</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.custom_start_date}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>End Date & Time</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.custom_end_date}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>IN</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.for_quantity}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>OUT</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.total_completed_qty}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-5\">\r\n                    <strong>NG</strong>\r\n                </div>\r\n                <div class=\"gj-col-7 text-right\"> \r\n                    {{doc.for_quantity - doc.total_completed_qty}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"section gj-p-1\">\r\n        <div class=\"gj-row gj-my-1\">\r\n            <div class=\"gj-col-5\">\r\n                <strong>Yield</strong>\r\n            </div>\r\n            <div class=\"gj-col-7 text-right\"> \r\n                {{((doc.total_completed_qty / doc.for_quantity) * 100) | format_currency}}%\r\n            </div>\r\n        </div>\r\n    </div>\r\n    {% if doc.scrap_items and doc.scrap_items | length > 0 %}\r\n    <div class=\"section gj-p-1\">\r\n        <div>\r\n            <strong>Defects</strong>\r\n        </div>\r\n        <div class=\"gj-row\">\r\n            <div class=\"gj-col-6\">\r\n                {% for item in doc.scrap_items %}\r\n                    <div>\r\n                        {{item.custom_defect}}\r\n                    </div>\r\n                {% endfor %}\r\n            </div>\r\n            <div class=\"gj-col-6 text-right\"> \r\n                {% for item in doc.scrap_items %}\r\n                    <div>\r\n                        {{item.stock_qty}}\r\n                    </div>\r\n                {% endfor %}\r\n            </div>\r\n        </div>\r\n    </div>\r\n    {% endif %}\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-6\">\r\n                    <strong>Status</strong>\r\n                </div>\r\n                <div class=\"gj-col-6 text-right\"> \r\n                    {{doc.status}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"gj-row\">\r\n        <div class=\"gj-col-12 gj-p-1\">\r\n            <div class=\"gj-row gj-my-1\">\r\n                <div class=\"gj-col-6\">\r\n                    <strong>Next Station</strong>\r\n                </div>\r\n                <div class=\"gj-col-6 text-right\"> \r\n                    {{doc.custom_next_station}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>",
